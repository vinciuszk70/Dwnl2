[1] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[2] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[3] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
},
[4] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
}
end))
end)()
end
end)()

coroutine.wrap(function()
for ReplicateSends = 1,3 do
coroutine.wrap(function()
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end)()
end
end)()

end

end))
end)()

end)

wait()

CrashForceServerDie = false

NewCmdLabel("Crash Force.")

end

