ChatBackColor = Color3.fromRGB(16,42,220)
ChatTextColor = Color3.fromRGB(0,0,0)
end

if Color == "red" then
ChatBackColor = Color3.fromRGB(255,0,0)
ChatTextColor = Color3.fromRGB(0,0,0)
end

if Color == "green" then
ChatBackColor = Color3.fromRGB(0, 255, 0)
ChatTextColor = Color3.fromRGB(0,0,0)
end

