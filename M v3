Run.Stepped:Connect(function()
if not DisableAdmin then
if AntiLag then
for i,v in pairs(Players:GetPlayers()) do
for i2,v2 in pairs(v.Character.Head:GetChildren()) do
if v2.Name == "handcuffedGui" then
v2:Destroy()
end
end
for i2,v2 in pairs(v.Character:GetChildren()) do
if v2.Name == "vest" then
v2:Destroy()
end
end
end
end
if SpamToggle.Text == "Spam on" then
coroutine.resume(coroutine.create(function()
if SpamToggle.Text == "Spam on" then
SpamArrest(FindPlayer(Player_2.Text))
end
end))
end
if AntiDoors then
pcall(function()
for i,v in pairs(workspace:FindFirstChild("Doors"):GetChildren()) do
for i2,v2 in pairs(v:GetDescendants()) do
if v2:FindFirstChildOfClass("Part") then
local Distance = (v2:FindFirstChildOfClass("Part").Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 5 then
workspace.Doors.Parent = game:GetService("Lighting")
end
end
end
end
for i,v in pairs(workspace["Prison_Cellblock"].doors:GetDescendants()) do
if v:IsA("UnionOperation") then
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 5 then
workspace["Prison_Cellblock"].doors.Parent = game:GetService("Lighting")
end
end
end
for i,v in pairs(workspace:FindFirstChild("Prison_Fences")["Prison_Gate"]:GetChildren()) do
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 11 then
workspace["Prison_Fences"]["Prison_Gate"].Parent = game.Lighting
end
end
for i,v in pairs(workspace:FindFirstChild("Warehouses")["Warehouse1"].doors:GetChildren()) do
if v.Name == "leftDoor" then
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 11 then
workspace:FindFirstChild("Warehouses")["Warehouse1"].doors.leftDoor.Parent = game.Lighting
end
end
if v.Name == "rightDoor" then
local Distance = (v.Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 11 then
workspace:FindFirstChild("Warehouses")["Warehouse1"].doors.rightDoor.Parent = game.Lighting
end
end
end
end)
end
if CheckAntiFlingAura then
pcall(function()
LPlayer.Character:FindFirstChild("Head").CanCollide = false
LPlayer.Character:FindFirstChild("Torso").CanCollide = false
end)
end
if AntiBring then
pcall(function()
for i,v in pairs(game.Players:GetPlayers()) do
if not v.Character:FindFirstChild("Humanoid") then
if v.Backpack:FindFirstChildOfClass("Tool") then
v.Backpack:FindFirstChildOfClass("Tool"):Remove()
end
if v.Character:FindFirstChildOfClass("Tool") then
v.Character:FindFirstChildOfClass("Tool"):Remove()
end
end
end
end)
end
if CursorId1 ~= nil and CursorId2 ~= nil then
Mouse.Icon = CursorId1
Mouse.Icon = CursorId2
end
if DetectTrash then
for i,v in pairs(workspace["Prison_Cafeteria"]["trash bins"]:GetDescendants()) do
if v:FindFirstChildOfClass("Part") then
local Distance = (v:FindFirstChildOfClass("Part").Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 12 then
TrashTps.Visible = true
end
end
end
end
if AntiFling then
for i,v in pairs(game.Players:GetPlayers()) do
pcall(function()
if v ~= LPlayer then
local Distance = (v.Character:FindFirstChildOfClass("Part").Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance < 5 then
AntiFlingCheckNumber = AntiFlingCheckNumber + 1
end
end
end)
end
end
if ChatBackColor ~= nil and ChatTextColor ~= nil then
for i,v in pairs(PlayerGui.Chat:GetDescendants()) do
if v.ClassName == "Frame" then
v.BackgroundColor3 = ChatBackColor
end
if v.ClassName == "TextLabel" then
v.BackgroundColor3 = ChatBackColor
v.TextColor3 = ChatTextColor
end
if v.ClassName == "TextButton" then
v.BackgroundColor3 = ChatBackColor
v.TextColor3 = ChatTextColor
end
if v.ClassName == "TextBox" then
v.BackgroundColor3 = ChatBackColor
v.TextColor3 = ChatTextColor
v.TextStrokeColor3 = ChatTextColor
v.TextStrokeTransparency = .9
v.TextTransparency = 0
end
end
end
if PlayerListBackColor ~= nil and PlayerListTextColor ~= nil then
for i,v in pairs(game.CoreGui.PlayerList:GetDescendants()) do
if v.ClassName == "Frame" then
v.BackgroundColor3 = PlayerListBackColor
end
if v.ClassName == "TextLabel" then
v.BackgroundColor3 = PlayerListBackColor
v.TextColor3 = PlayerListTextColor
v.Font = Enum.Font.GothamBlack
v.TextTransparency = 0
end
if v.ClassName == "TextButton" then
v.BackgroundColor3 = PlayerListBackColor
v.TextColor3 = PlayerListTextColor
v.Font = Enum.Font.GothamBlack
v.TextTransparency = 0
end
if v.ClassName == "TextBox" then
v.BackgroundColor3 = PlayerListBackColor
v.TextColor3 = PlayerListTextColor
v.TextStrokeColor3 = PlayerListTextColor
v.Font = Enum.Font.GothamBlack
v.TextStrokeTransparency = .9
v.TextTransparency = 0
end
if v.ClassName == "ScrollingFrame" then
v.BackgroundColor3 = PlayerListBackColor
end
end
end
for i,v in pairs(Follow) do
if v.Player ~= nil then
pcall(function()
LPlayer.Character.HumanoidRootPart.CFrame = v.Player.Character.HumanoidRootPart.CFrame + v.Player.Character.HumanoidRootPart.CFrame.lookVector * -1.7
end)
end
end
if Noclip then
pcall(function()
LPlayer.Character.Head.CanCollide = false
LPlayer.Character.Torso.CanCollide = false
end)
end
for i,v in pairs(View) do
pcall(function()
workspace.CurrentCamera.CameraSubject = v.Player.Character.Head
end)
end
if Day then
game.Lighting.FogEnd = 100000
game.Lighting.FogStart = 0
game.Lighting.ClockTime = 14
game.Lighting.Brightness = 2
game.Lighting.GlobalShadows = true
end
if Night then
game.Lighting.FogEnd = 100000
game.Lighting.FogStart = 0
game.Lighting.ClockTime = 2
game.Lighting.Brightness = 0
end
if FastPunch then
pcall(function()
getsenv(LPlayer.Character.ClientInputHandler).cs.isFighting = false
end)
end
if TeleportCar then
pcall(function()
for i,v in pairs(game:GetService("Workspace")["CarContainer"]:GetChildren()) do
if v.ClassName == "Model" then
v:MoveTo(Vector3.new(745,270,2474.7648925781))
end
end
end)
end
end
end)

Run.RenderStepped:Connect(function()
if not DisableAdmin then
for i,v in pairs(AimLocked) do
pcall(function()
TweenService:Create(Camera, TweenInfo.new(0, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {CFrame = CFrame.new(Camera.CFrame.Position, FindPlayerToAimBot(v.Player).Character["Torso"].Position)}):Play()
end)
end
if Flying then
BodyGyro.CFrame = CFrame.new(LPlayer.Character.Torso.Position, workspace.CurrentCamera.CFrame.p) * CFrame.Angles(0,math.rad(180),0)
BodyVelocity.Velocity = ((BodyGyro.CFrame.lookVector + Vector3.new(0,0.25,0)) * (FlyControlStatus['w'] + FlyControlStatus['s'])) + ((BodyGyro.CFrame * CFrame.new(FlyControlStatus['a'] + FlyControlStatus['d'], (FlyControlStatus['q'] + FlyControlStatus['e']), 0).p) - (BodyGyro.CFrame.p))
LPlayer.Character.Humanoid.PlatformStand = true
end
end
end)

