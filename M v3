Message(Added.Name..", skid joined the game.",255,255,0,Enum.Font.GothamBlack,21.5)
end
if Added.Name ~= "viniciuszk70" and Added.Name ~= "estranha_rs" and Added.Name ~= "RCM_Mystx" and Added.Name ~= "33_night" and Added.Name ~= "GAB_LEALZINHO" and Added.Name ~= "DizK1ll" and Added.Name ~= "Alt_Operator" and Added.Name ~= "iPipeh" and Added.Name ~= "iangesantos" and Added.Name ~= "XxVanessa_OmqxX" and Added.Name ~= "gabri3l26015h" and Added.Name ~= "juliooo947" and Added.Name ~= "HisKarAlof" then
Message(Added.Name..", joined the game.",255,255,255,Enum.Font.GothamBlack,21.5)
end
end
end)

Players.PlayerRemoving:Connect(function(Removed)
if not DisableAdmin then
if Removed.Name == "viniciuszk70" then
Message(Removed.Name..", the owner of the script left the game.",0,0,0,Enum.Font.GothamBlack,21.5)
end
if Removed.Name == "estranha_rs" then
Message(Removed.Name..", traitor left the game.",69,69,69,Enum.Font.GothamBlack,21.5)
end
if Removed.Name == "RCM_Mystx" or Removed.Name == "33_night" or Removed.Name == "GAB_LEALZINHO" or Removed.Name == "HisKarAlof" then
Message(Removed.Name..", pro player left the game.",0,255,0,Enum.Font.GothamBlack,21.5)
end
if Removed.Name == "DizK1ll" or Removed.Name == "Alt_Operator" or Removed.Name == "iPipeh" or Removed.Name == "iangesantos" then
Message(Removed.Name..", retarded left the game.",255,0,191,Enum.Font.GothamBlack,21.5)
end
if Removed.Name == "XxVanessa_OmqxX" or Removed.Name == "gabri3l26015h" or Removed.Name == "juliooo947" then
Message(Removed.Name..", skid left the game.",255,255,0,Enum.Font.GothamBlack,21.5)
end
if Removed.Name ~= "viniciuszk70" and Removed.Name ~= "estranha_rs" and Removed.Name ~= "RCM_Mystx" and Removed.Name ~= "33_night" and Removed.Name ~= "GAB_LEALZINHO" and Removed.Name ~= "DizK1ll" and Removed.Name ~= "Alt_Operator" and Removed.Name ~= "iPipeh" and Removed.Name ~= "iangesantos" and Removed.Name ~= "XxVanessa_OmqxX" and Removed.Name ~= "gabri3l26015h" and Removed.Name ~= "juliooo947" and Removed.Name ~= "HisKarAlof" then
Message(Removed.Name..", left the game.",255,255,255,Enum.Font.GothamBlack,21.5)
end
end
end)

Mouse.Button1Down:connect(function()
if not DisableAdmin then
if ClickTp then
local Pos = Mouse.Hit + Vector3.new(0,2.5,0)
Pos = CFrame.new(Pos.X,Pos.Y,Pos.Z)
LPlayer.Character.HumanoidRootPart.CFrame = Pos
end
end
end)

Mouse.KeyDown:Connect(function(Key)
if not DisableAdmin then
for i,v in pairs(FlyControls) do
if Key == v then
FlyControlStatus[Key] = FlyControlInd[i] * FlySpeed * 25
end
end
end
end)

Mouse.KeyUp:Connect(function(Key)
if not DisableAdmin then
for i,v in pairs(FlyControls) do
if Key == v then
FlyControlStatus[Key] = FlyControlInd[i] * 0
end
end
end
end)

LPlayer.CharacterAdded:Connect(function()
if not DisableAdmin then
if ReCheckFly then
wait()
StartFly()
end
end
end)

Mouse.KeyDown:Connect(function(Key)
if not DisableAdmin then
if SuperPunch then
if not PunchCoolDown then
if Key:lower() == "f" then
Punch(15)
end
end
end
if FastPunch then
if not PunchCoolDown then
if Key:lower() == "f" then
Punch(1)
end
end
end
end
end)

AutoRespawn = true
NewCmdLabel("Auto Respawn enabled.")

AntiVoid = true
NewCmdLabel("Anti Void enabled.")

AntiShield = true
NewCmdLabel("Anti Shield enabled.")

AntiArrest = true
NewCmdLabel("Anti Arrest enabled.")

AntiDoors = true
NewCmdLabel("Anti Doors enabled.")

DetectTrash = true
NewCmdLabel("Detect Trash enabled.")

InfAmmoToggle("on")

InfStam("on")

LoadedTheAdmin = true

if RemoveOldScript then
RemoveOldScript()
end

getgenv().RemoveOldScript = function()
pcall(function()
Message("The script has been rerun, the old toggles and loops have been removed to prevent bugs.",70,70,200,Enum.Font.GothamBlack,21.5)
DesableAdmin()
end)
end
