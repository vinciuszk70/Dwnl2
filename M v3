ChatTextColor = Color3.fromRGB(0,0,0)
end

if Color == "orange" then
ChatBackColor = Color3.fromRGB(255,176,0)
ChatTextColor = Color3.fromRGB(0,0,0)
end

if Color == "grey" then
ChatBackColor = Color3.fromRGB(87,88,87)
ChatTextColor = Color3.fromRGB(0,0,0)
end

if ChatBackColor ~= nil and ChatTextColor ~= nil then
NewCmdLabel("New Chat Color.")
end

end

