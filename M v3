function TerrainColor(Color)

if Color == "white" then
BlockColor = BrickColor.new("1001")
end

if Color == "black" then
BlockColor = BrickColor.new("26")
end

if Color == "blue" then
BlockColor = BrickColor.new("102")
end

if Color == "red" then
BlockColor = BrickColor.new("331")
end

if Color == "green" then
BlockColor = BrickColor.new("318")
end

if Color == "yellow" then
BlockColor = BrickColor.new("226")
end

if Color == "purple" then
BlockColor = BrickColor.new("321")
end

if Color == "pink" then
BlockColor = BrickColor.new("330")
end

if Color == "orange" then
BlockColor = BrickColor.new("123")
end

if Color == "grey" then
BlockColor = BrickColor.new("148")
end

if BlockColor ~= nil then

pcall(function()
for i,v in pairs(workspace["GAME_TERRAIN"]["Prison_Grounds"]:GetDescendants()) do
if v.ClassName == "Part" then
v.BrickColor = BlockColor
end
end
for i,v in pairs(Workspace["GAME_TERRAIN"]:GetDescendants()) do
if v.Name == "Wedge" then
v.BrickColor = BlockColor
end
end
for i,v in pairs(Workspace["GAME_TERRAIN"]:GetDescendants()) do
if v.ClassName == "Part" then
v.BrickColor = BlockColor
end
end
for i,v in pairs(workspace["Prison_Trees"]["Front"]:GetDescendants()) do
if v.ClassName == "Part" then
v.BrickColor = BlockColor
end
end
for i,v in pairs(workspace["Prison_Trees"]["Back"]:GetDescendants()) do
if v.ClassName == "Part" then
v.BrickColor = BlockColor
end
end
for i,v in pairs(workspace["Trees"]:GetDescendants()) do
if v.ClassName == "Part" then
v.BrickColor = BlockColor
end
if v.ClassName == "MeshPart" then
v.BrickColor = BlockColor
end
end
for i,v in pairs(workspace["Square trees"]:GetDescendants()) do
if v.ClassName == "Part" then
v.BrickColor = BlockColor
end
end
end)

NewCmdLabel("New Terrain Color.")

end

end

