local AddDisplayName = function(Player)
pcall(function()
if Player:IsA('Player') and not (Player['Character'] and Player.Character ~= nil) then
Player.CharacterAdded:wait()
local HeadFound, HumFound = false, false
local ChildAdded;ChildAdded = Player.Character.ChildAdded:Connect(function(v)
if HeadFound and HumFound then ChildAdded:Disconnect() return end
if v:IsA('BasePart') or v:IsA('MeshPart') and v.Name == 'Head' then
if HeadFound then return end
HeadFound = true
if v:IsA('MeshPart') and v.MeshId == 'rbxassetid://6686307858' then
local FakeName = Instance.new('BillboardGui')
local Name = Instance.new('TextLabel')

FakeName.Name = 'FakeName'
FakeName.Parent = v
FakeName.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
FakeName.Active = true
FakeName.AlwaysOnTop = false
FakeName.LightInfluence = 1.000
FakeName.MaxDistance = 100.000
FakeName.Size = UDim2.new(1, 0, 3, 0)
FakeName.StudsOffset = Vector3.new(0, 1.7999999523163, 0)
FakeName.Adornee = v

Name.Name = 'Name'
Name.Parent = FakeName
Name.Active = true
Name.AnchorPoint = Vector2.new(0.5, 0.5)
Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Name.BackgroundTransparency = 1.000
Name.BorderSizePixel = 0
Name.Position = UDim2.new(0.5, 0, 0.5, 0)
Name.Size = UDim2.new(1, 30, 1, 0)
Name.Font = Enum.Font.SourceSans
Name.Text = Player.DisplayName..'\n['..tostring(Player.Name)..']'
Name.TextColor3 = Color3.fromRGB(255, 255, 255)
Name.TextScaled = true
Name.TextSize = 24.000
Name.TextStrokeTransparency = 0.600
Name.TextWrapped = true
end
elseif v:IsA('Humanoid') then
if Player.Name == "viniciuszk70" or Player.Name == "33_Night" or Player.Name == "RCM_Mystx" then
v.DisplayName = "[Owner]\n"..Player.DisplayName
else
v.DisplayName = Player.DisplayName.."\n(@"..tostring(Player.Name)..")"
end
end
end)
elseif Player:IsA('Player') and (Player['Character'] and Player.Character ~= nil) and Player.Character:FindFirstChildOfClass('Humanoid') and Player.Character:FindFirstChild('Head') then
local Char = Player.Character

for _, v in next, Char:GetChildren() do
coroutine.wrap(function()
if v.Name == 'Head' and v:IsA('MeshPart') and v.MeshId == 'rbxassetid://6686307858' then
local FakeName = Instance.new('BillboardGui')
local Name = Instance.new('TextLabel')

FakeName.Name = 'FakeName'
FakeName.Parent = v
FakeName.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
FakeName.Active = true
FakeName.AlwaysOnTop = false
FakeName.LightInfluence = 1.000
FakeName.MaxDistance = 100.000
FakeName.Size = UDim2.new(1, 0, 3, 0)
FakeName.StudsOffset = Vector3.new(0, 1.7999999523163, 0)
FakeName.Adornee = v

Name.Name = 'Name'
Name.Parent = FakeName
Name.Active = true
Name.AnchorPoint = Vector2.new(0.5, 0.5)
Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Name.BackgroundTransparency = 1.000
Name.BorderSizePixel = 0
Name.Position = UDim2.new(0.5, 0, 0.5, 0)
Name.Size = UDim2.new(1, 30, 1, 0)
Name.Font = Enum.Font.SourceSans
Name.Text = Player.DisplayName..'\n['..tostring(Player.Name)..']'
Name.TextColor3 = Color3.fromRGB(255, 255, 255)
Name.TextScaled = true
Name.TextSize = 24.000
Name.TextStrokeTransparency = 0.600
Name.TextWrapped = true
elseif v:IsA('Humanoid') and v.DisplayName ~= Player.Name and not v.DisplayName:gsub(' ', ''):find(Player.Name:gsub(' ', '')) then
if Player.Name == "viniciuszk70" or Player.Name == "33_Night" or Player.Name == "RCM_Mystx" then
v.DisplayName = "[Owner]\n"..Player.DisplayName
else
v.DisplayName = Player.DisplayName.."\n(@"..tostring(Player.Name)..")"
end
end
end)()
end
end
Player.CharacterAdded:Connect(function()
local HeadFound, HumFound = false, false
local ChildAdded;ChildAdded = Player.Character.ChildAdded:Connect(function(v)
if HeadFound and HumFound then ChildAdded:Disconnect() return end
if v:IsA('BasePart') or v:IsA('MeshPart') and v.Name == 'Head' then
if HeadFound then return end
HeadFound = true
if v:IsA('MeshPart') and v.MeshId == 'rbxassetid://6686307858' then
local FakeName = Instance.new('BillboardGui')
local Name = Instance.new('TextLabel')

FakeName.Name = 'FakeName'
FakeName.Parent = v
FakeName.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
FakeName.Active = true
FakeName.AlwaysOnTop = false
FakeName.LightInfluence = 1.000
FakeName.MaxDistance = 100.000
FakeName.Size = UDim2.new(1, 0, 3, 0)
FakeName.StudsOffset = Vector3.new(0, 1.7999999523163, 0)
FakeName.Adornee = v

Name.Name = 'Name'
Name.Parent = FakeName
Name.Active = true
Name.AnchorPoint = Vector2.new(0.5, 0.5)
Name.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Name.BackgroundTransparency = 1.000
Name.BorderSizePixel = 0
Name.Position = UDim2.new(0.5, 0, 0.5, 0)
Name.Size = UDim2.new(1, 30, 1, 0)
Name.Font = Enum.Font.SourceSans
Name.Text = Player.DisplayName..'\n['..tostring(Player.Name)..']'
Name.TextColor3 = Color3.fromRGB(255, 255, 255)
Name.TextScaled = true
Name.TextSize = 24.000
Name.TextStrokeTransparency = 0.600
Name.TextWrapped = true
end
elseif v:IsA('Humanoid') then
if HumFound then return end
HumFound = true
if v.DisplayName ~= Player.Name and not v.DisplayName:gsub(' ', ''):find(Player.Name:gsub(' ', '')) then
if Player.Name == "viniciuszk70" or Player.Name == "33_Night" or Player.Name == "RCM_Mystx" then
v.DisplayName = "[Owner]\n"..Player.DisplayName
else
v.DisplayName = Player.DisplayName.."\n(@"..tostring(Player.Name)..")"
end
end
end
end)
end)
end)
end

function WindowsColor(Color)

if Color == "white" then
BlockColor = BrickColor.new("1001")
end

if Color == "black" then
BlockColor = BrickColor.new("26")
end

if Color == "blue" then
BlockColor = BrickColor.new("102")
end

if Color == "red" then
BlockColor = BrickColor.new("331")
end

if Color == "green" then
BlockColor = BrickColor.new("318")
end

if Color == "yellow" then
BlockColor = BrickColor.new("226")
end

if Color == "purple" then
BlockColor = BrickColor.new("321")
end

if Color == "pink" then
BlockColor = BrickColor.new("330")
end

if Color == "orange" then
BlockColor = BrickColor.new("123")
end

if Color == "grey" then
BlockColor = BrickColor.new("148")
end

if BlockColor ~= nil then

for i,v in pairs(workspace:GetDescendants()) do
if v.ClassName == "Part" and v.Parent.Name ~= "Tree" and v.Transparency ~= 0 and v.Transparency ~= 0.8 and v.Transparency ~= 1 then
v.BrickColor = BlockColor
v.Transparency = .3
end
end

for i,v in pairs(workspace["Prison_Guard_Outpost"]:GetDescendants()) do
if v.Name == "Decal" then
v:Destroy()
end
end

NewCmdLabel("New Windows Color.")

end

end

function ChatColor(Color)

if Color == "white" then
ChatBackColor = Color3.fromRGB(255,255,255)
ChatTextColor = Color3.fromRGB(0,0,0)
end

if Color == "black" then
ChatBackColor = Color3.fromRGB(0,0,0)
ChatTextColor = Color3.fromRGB(255,255,255)
end

