if Color == "white" then
ColorGun = "1001"
end

if Color == "black" then
ColorGun = "1003"
end

if Color == "blue" then
ColorGun = "307"
end

if Color == "red" then
ColorGun = "21"
end

if Color == "green" then
ColorGun = "1020"
end

if Color == "yellow" then
ColorGun = "1009"
end

if Color == "purple" then
ColorGun = "104"
end

if Color == "pink" then
ColorGun = "1032"
end

if Color == "orange" then
ColorGun = "123"
end

if Color == "grey" then
ColorGun = "148"
end

if Color ~= nil then
NewCmdLabel("CGun enabled.")
end

if Color == "off" then
ColorGun = nil
Cmds.Respawn()
NewCmdLabel("CGun desabled.")
end

end

local function FindPlayerToAimBot(Player)
local MaximumDistance = math.huge
local Target = nil
coroutine.wrap(function()
wait(20); MaximumDistance = math.huge
end)()
if Player.Character ~= nil then
if Player.Character:FindFirstChild("HumanoidRootPart") ~= nil then
if Player.Character:FindFirstChild("Humanoid") ~= nil and Player.Character:FindFirstChild("Humanoid").Health ~= 0 then
local ScreenPoint = Camera:WorldToScreenPoint(Player.Character:WaitForChild("HumanoidRootPart", math.huge).Position)
local VectorDistance = (Vector2.new(UserInputService:GetMouseLocation().X, UserInputService:GetMouseLocation().Y) - Vector2.new(ScreenPoint.X, ScreenPoint.Y)).Magnitude
if VectorDistance < MaximumDistance then
Target = Player
MaximumDistance = VectorDistance
end
end
end
end
return Target
end

