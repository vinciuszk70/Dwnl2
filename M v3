function StartFly()

pcall(function()
BodyGyro:Destroy()
BodyVelocity:Destroy()
BodyGyro = nil
BodyVelocity = nil
end)

pcall(function()
BodyGyro = Instance.new("BodyGyro", LPlayer.Character.Torso)
BodyGyro.P = 0
BodyGyro.MaxTorque = Vector3.new(0,0,0)
BodyGyro.CFrame = LPlayer.Character.Torso.CFrame

BodyVelocity = Instance.new("BodyVelocity", LPlayer.Character.Torso)
BodyVelocity.MaxForce = Vector3.new(0,0,0)
BodyVelocity.Velocity = Vector3.new(0,0,0)

Flying = true
BodyGyro.P = 9e4
BodyGyro.MaxTorque = Vector3.new(9e4,9e4,9e4)
BodyGyro.CFrame = LPlayer.Character.Torso.CFrame

BodyVelocity.MaxForce = Vector3.new(9e4,9e4,9e4)
BodyVelocity.Velocity = Vector3.new(0,0,0)
end)

end

function EndFly()

pcall(function()
Flying = false

BodyGyro.P = 0
BodyGyro.MaxTorque = Vector3.new(0,0,0)
BodyGyro.CFrame = LPlayer.Character.Torso.CFrame

BodyVelocity.MaxForce = Vector3.new(0,0,0)
BodyVelocity.Velocity = Vector3.new(0,0,0)
LPlayer.Character.Humanoid.PlatformStand = false
BodyGyro:Destroy()
BodyVelocity:Destroy()
BodyGyro = nil
BodyVelocity = nil
end)

end

function DesableAdmin()
HitBox = false
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer then
pcall(function()
v.Character.HumanoidRootPart.Size = Vector3.new(2, 2, 1)
v.Character.HumanoidRootPart.Transparency = 1
v.Character.HumanoidRootPart.Material = "Plastic"
v.Character.HumanoidRootPart.CanCollide = true
end)
end
end
ESP = false
pcall(function()
for i,v in pairs(Players:GetPlayers()) do
for i2,v2 in pairs(v.Character:GetChildren()) do
if v ~= LPlayer then
if v2:FindFirstChild("BoxHandle") then
v2.BoxHandle:Destroy()
end
end
end
end
end)
if ReCheckFly or Flying then
ReCheckFly = false
EndFly()
end
Cmds.Respawn()
MsdAdminV2:Destroy()
ThemesGui:Destroy()
DisableAdmin = true
RemoveOldScript:Destroy()
end

function Times(Time)

if Time == "day" or Time == "night" or Time == "off" then

if Time == "day" then

if Night then
Night = false
end

Day = true

end

if Time == "night" then

if Day then
Day = false
end

Night = true

end

if Time == "off" then

if Day or Night then

Day = false
Night = false

end

end

NewCmdLabel("New Time")

end

end

function Punch(Power)

pcall(function()

PunchCoolDown = true

local PunchPart = Instance.new("Part",LPlayer.Character)
PunchPart.Transparency = 1
PunchPart.Size = Vector3.new(6,6,4)
PunchPart.CanCollide = false

local PunchPart_2 = Instance.new("Weld",PunchPart)
PunchPart_2.Part0 = LPlayer.Character.Torso
PunchPart_2.Part1 = PunchPart
PunchPart_2.C1 = CFrame.new(0,0,2)

PunchPart.Touched:connect(function(Hit)
if Players:FindFirstChild(Hit.Parent.Name) then
local Player = Players:FindFirstChild(Hit.Parent.Name) 
if Player.Name ~= LPlayer.Name then
PunchPart:Destroy()

for i = 1,Power do
MeleeEvent:FireServer(Player)
end
end
end
end)

wait(.5)

PunchCoolDown = false

PunchPart:Destroy()

end)

end

function BringCars()
pcall(function()
for i,v in next, getgc() do
if type(v) == "function" and getfenv(v).script and getfenv(v).script == LPlayer.Character.ClientInputHandler then 
for i2,v2 in next, debug.getupvalues(v) do 
if type(v2) == "number" then 
debug.setupvalue(v, i2, 10000)
end
end
end
end
for i,v in pairs(game:GetService("Workspace")["Prison_ITEMS"].buttons:GetDescendants()) do
if v.Parent.Name == "Car Spawner" then
if v.Name == "Car Spawner" then
workspace.Remote.ItemHandler:InvokeServer(v)
end
end
end
wait(1)
TeleportCar = true
wait(2)
TeleportCar = false
for i,v in pairs(game:GetService("Workspace")["CarContainer"]:GetDescendants()) do
if v.ClassName == "Seat" then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
end
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
for i,v in pairs(game:GetService("Workspace")["CarContainer"]:GetDescendants()) do
if v.ClassName == "Seat" then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
end
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
end
end
wait(.3)
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
wait()
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(760,100,2474.7648925781)
else
wait()
LPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(760,100,2474.7648925781)
end
end)
end

function NewCmdLabel(Text)
local Clone = CmdLabel:Clone()
Clone.Text = Text
Clone.Name = "New Cmd"
Clone.Parent = NotifyScrollingFrame
NotifyCanvasSize = NotifyCanvasSize  + .3
NotifyScrollingFrame.CanvasSize = UDim2.new(0, 0, NotifyCanvasSize, 0)
end

function Message(Text,ColorD,ColorT,ColorQ,Font,TextSize)

StarterGui:SetCore("ChatMakeSystemMessage",{
Text = Text,
Color = Color3.fromRGB(ColorD,ColorT,ColorQ);
Font = Font;
TextSize = TextSize;
})

end

function Args(Msg)
Split = Msg:split(" ")
Arg1 = Split[1]
Arg2 = Split[2]
Arg3 = Split[3]
Arg4 = Split[4]
Arg5 = Split[5]
Arg6 = Split[6]
Arg7 = Split[7]
Arg8 = Split[8]
Arg9 = Split[9]
Arg10 = Split[10]
end

Connect.SendCommand = function(Msg)
return Arg1 == Prefix..Msg
end

Connect.Commands = function(Msg)

if not DisableAdmin then

Args(Msg)

if Connect.SendCommand("to") then
Cmds.Goto(FindPlayer(Arg2))
NewCmdLabel("Teleported.")
end

if Connect.SendCommand("sagui") then
SpamGui.Visible = true
NewCmdLabel("Spam Gui enabled.")
end

if Connect.SendCommand("k") or Connect.SendCommand("kill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.KillTeam(Arg2:lower())

else

Cmds.Kill(FindPlayer(Arg2))

end
end

if Connect.SendCommand("lk") or Connect.SendCommand("loopkill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.LoopKill(Arg2:lower())

else

local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and not Lk[Player.UserId] then
Lk[Player.UserId] = {Player = Player}
NewCmdLabel("Lk "..Player.Name.." added.")
end

end
end

if Connect.SendCommand("unlk") or Connect.SendCommand("unloopkill") then
if Arg2 == "i" or Arg2 == "inmates" or Arg2 == "g" or Arg2 == "guards" or Arg2 == "n" or Arg2 == "neutros" or Arg2 == "l" or Arg2 == "neutrals" or Arg2 == "c" or Arg2 == "criminals" or Arg2 == "a" or Arg2 == "all" then

Cmds.UnLoopKill(Arg2:lower())

else

local Player = FindPlayer(Arg2)
if Player ~= nil and Player.Name ~= "viniciuszk70" and Lk[Player.UserId] then
Lk[Player.UserId] = nil
NewCmdLabel("Lk "..Player.Name.." removed.")
end

end
end

if Connect.SendCommand("t") or Connect.SendCommand("team") then
if Arg2 == "i" or Arg2 == "g" or Arg2 == "c" or Arg2 == "n" or Arg2 == "white" or Arg2 == "black" or Arg2 == "blue" or Arg2 == "red" or Arg2 == "green" or Arg2 == "purple" or Arg2 == "grey" or Arg2 == "pink" or Arg2 == "yellow" or Arg2 == "orange" then

TeamColor(Arg2:lower())

NewCmdLabel("New Team Selected")

else

for i,v in pairs(BrickColors) do
if Arg3 then
if v == Arg2.." "..Arg3 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Arg2.." "..Arg3)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
NewCmdLabel("New Team Selected")
end
else
if v == Arg2 then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer(nil, Arg2)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
NewCmdLabel("New Team Selected")
end
end
end

end
end

if Connect.SendCommand("re") then
Cmds.Respawn()
end

if Connect.SendCommand("auto") then
if Arg2 == "on" then
if not AutoRespawn then
AutoRespawn = true
NewCmdLabel("Auto Respawn enabled.")
end
end
if Arg2 == "off" then
if AutoRespawn then
AutoRespawn = false
NewCmdLabel("Auto Respawn desabled.")
end
end
if Arg2 == nil then
if not AutoRespawn then
AutoRespawn = true
NewCmdLabel("Auto Respawn enabled.")
end
end
end

if Connect.SendCommand("unauto") then
if AutoRespawn then
AutoRespawn = false
NewCmdLabel("Auto Respawn desabled.")
end
end

if Connect.SendCommand("autolock") then
if Arg2 == "on" then
if not AutoLock then
AutoLock = true
NewCmdLabel("Auto Lock enabled.")
end
end
if Arg2 == "off" then
if AutoLock then
AutoLock = false
NewCmdLabel("Auto Lock desabled.")
Cmds.Respawn()
end
end
if Arg2 == nil then
if not AutoLock then
AutoLock = true
NewCmdLabel("Auto Lock enabled.")
end
end
end

if Connect.SendCommand("unautolock") then
if AutoLock then
AutoLock = false
NewCmdLabel("Auto Lock desabled.")
Cmds.Respawn()
end
end

