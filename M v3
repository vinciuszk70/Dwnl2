NewCmdLabel("Auto Guns enabled.")

end

end

end

end

function BringT(TP)

pcall(function()

if TP == "i" or TP == "inmates" or TP == "g" or TP == "guards" or TP == "n" or TP == "neutros" or TP == "l" or TP == "neutrals" or TP == "c" or TP == "criminals" or TP == "a" or TP == "all" then

if TP == "i" or TP == "inmates" then
BringNormal = true
TS = game.Teams.Inmates
NewCmdLabel("Bring Inmates.")
end

if TP == "g" or TP == "guards" then
BringNormal = true
TS = game.Teams.Guards
NewCmdLabel("Bring Guards.")
end

if TP == "n" or TP == "neutros" then
BringNormal = true
TS = game.Teams.Neutral
NewCmdLabel("Bring Neutros.")
end

if TP == "l" or TP == "neutrals" then
BringNormal = false
BringNeutrals = true
NewCmdLabel("Bring Neutrals.")
end

if TP == "c" or TP == "criminals" then
BringNormal = true
TS = game.Teams.Criminals
NewCmdLabel("Bring Criminals.")
end

if TP == "a" or TP == "all" then
BringNormal = true
TS = game.Players
NewCmdLabel("Bring All.")
end

if BringNormal then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
for i,v in pairs(TS:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if v.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Sit = false
end

if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local C = LPlayer.Character["1"]:Clone()
C.Name = "Humanoid"
C.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local STOP = 0
repeat wait(.1)
STOP = STOP + 1
LPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not v.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not v.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
local STOP_2 = 0
repeat wait()
STOP_2 = STOP_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
until STOP_2 > 10
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end

if BringNeutrals then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" then
if v.TeamColor.Name ~= "Bright orange" and v.TeamColor.Name ~= "Medium stone grey" and v.TeamColor.Name ~= "Bright blue" and v.TeamColor.Name ~= "Really red" then
if v.Character:FindFirstChild("HumanoidRootPart") then
if v ~= LPlayer and v.Name ~= "viniciuszk70" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local i = 1
repeat wait()
i = i-1
if v.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Sit = false
end

if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local C = LPlayer.Character["1"]:Clone()
C.Name = "Humanoid"
C.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local STOP = 0
repeat wait(.1)
STOP = STOP + 1
LPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not v.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not v.Character.HumanoidRootPart.Parent or STOP > 500) and STOP > 3
local STOP_2 = 0
repeat wait()
STOP_2 = STOP_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
until STOP_2 > 10
workspace.Remote.loadchar:InvokeServer()
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
end
end
end

end
end)
end

function Bring(Player,Position)
pcall(function()
if Player == nil and Player.Name ~= "viniciuszk70" and Position == nil then return end
if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.Random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
else
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer()
if LPlayer.TeamColor.Name == "Bright orange" then
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
else
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.Humanoid.Name = "1"
local HumanoidClone = LPlayer.Character["1"]:Clone()
HumanoidClone.Name = "Humanoid"
HumanoidClone.Parent = LPlayer.Character
LPlayer.Character["1"]:Destroy()
game.Workspace.CurrentCamera.CameraSubject = LPlayer.Character
LPlayer.Character.Animate.Disabled = true
wait()
LPlayer.Character.Animate.Disabled = false
LPlayer.Character.Humanoid.DisplayDistanceType = "None"
LPlayer:FindFirstChild("Backpack"):FindFirstChild("Hammer").Parent = LPlayer.Character
local Count = 0
repeat wait(.1)
Count = Count + 1
LPlayer.Character.HumanoidRootPart.CFrame = Player.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 0.75)
until (not LPlayer.Character:FindFirstChild("Hammer") or not LPlayer.Character.HumanoidRootPart or not Player.Character.HumanoidRootPart or not LPlayer.Character.HumanoidRootPart.Parent or not Player.Character.HumanoidRootPart.Parent or Player.Character.Humanoid.Health == 0 or Count > 500) and Count > 3
local Count_2 = 0
repeat wait()
Count_2 = Count_2 + 1
LPlayer.Character.HumanoidRootPart.CFrame = Position
until Count_2 > 10
workspace.Remote.loadchar:InvokeServer()
if LPlayer.TeamColor.Name == "Bright orange" then
wait(.1)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
else
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end)
end

function CrimT(TP)

pcall(function()

if TP == "i" or TP == "inmates" or TP == "g" or TP == "guards" or TP == "n" or TP == "neutros" or TP == "l" or TP == "neutrals" or TP == "a" or TP == "all" then

if TP == "i" or TP == "inmates" then
CrimNormal = true
TS = game.Teams.Inmates
NewCmdLabel("Crim Inmates.")
end

if TP == "g" or TP == "guards" then
CrimNormal = true
TS = game.Teams.Guards
NewCmdLabel("Crim Guards.")
end

if TP == "n" or TP == "neutros" then
CrimNormal = true
TS = game.Teams.Neutral
NewCmdLabel("Crim Neutros.")
end

if TP == "l" or TP == "neutrals" then
CrimNormal = false
CrimNeutrals = true
NewCmdLabel("Crim Neutrals.")
end

if TP == "a" or TP == "all" then
CrimNormal = true
TS = game.Players
NewCmdLabel("Crim All.")
end

if CrimNormal then
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
for i,v in pairs(TS:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" and v.TeamColor.Name ~= "Really red" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if v ~= nil then
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = v.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = v.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
end
end

if CrimNeutrals then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" then
if v.TeamColor.Name ~= "Bright orange" and v.TeamColor.Name ~= "Medium stone grey" and v.TeamColor.Name ~= "Bright blue" and v.TeamColor.Name ~= "Really red" then
if v.Character:FindFirstChild("HumanoidRootPart") then
if v ~= LPlayer and v.Name ~= "viniciuszk70" and v.TeamColor.Name ~= "Really red" and v.Character.Humanoid.Health ~= 0 and v.Character.Humanoid.Sit ~= true then
local i = 1
repeat wait()
i = i-1
if LPlayer.TeamColor.Name == "Bright blue" then
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

if v ~= nil then
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = v.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = v.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
until i == 0
end
end
end
end
end
end

end
end)
end

function Crim(Player, mebitch)
pcall(function()
local Player = FindPlayer(Arg2)

NewCmdLabel("Crim "..Player.Name..".")

if Player ~= nil and Player.Name ~= "viniciuszk70" then

if LPlayer.TeamColor.Name == "Bright blue" then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, "12")
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
workspace.Remote.loadchar:InvokeServer()
ItemHandler:InvokeServer(ItemSingle["Hammer"].ITEMPICKUP)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
function Wait(t)
local RS = game:GetService("RunService")
if type(t) == "number" then
local Start = tick()
repeat RS.Stepped:Wait() until tick() - Start >= t
return
else
return(RS.Stepped:Wait())
end  
end
Wait()
local hrp = LPlayer.Character.HumanoidRootPart
local char = LPlayer.Character
local tchar = Player.Character
local hum = LPlayer.Character:FindFirstChildOfClass("Humanoid")
local hrp = LPlayer.Character.HumanoidRootPart
local hrp2 = Player.Character.HumanoidRootPart
hum.Name = "1"
local newHum = hum:Clone()
newHum.Parent = char
newHum.Name = "Humanoid"
hum:Destroy()
workspace.CurrentCamera.CameraSubject = char
newHum.DisplayDistanceType = "None"
local tool = LPlayer:FindFirstChildOfClass("Backpack"):FindFirstChild("Hammer") or LPlayer.Character:FindFirstChild("Hammer")
tool.Parent = char
hrp.CFrame = hrp2.CFrame * CFrame.new(0, 0, 0) * CFrame.new(math.random(-100, 100)/200,math.random(-100, 100)/200,math.random(-100, 100)/200)
local n = 0
repeat
Wait()
n = n + 1
hrp.CFrame = hrp2.CFrame
until (tool.Parent ~= char or not hrp or not hrp2 or not hrp.Parent or not hrp2.Parent or n > 250) and n > 2
for i = 1,10 do

hrp.CFrame = SaveFrame
end
dfp = game.Workspace["Criminals Spawn"].SpawnLocation
local PartPos = dfp.CFrame
dfp.CanCollide = false
dfp.Transparency = 1
dfp.CFrame = LPlayer.Character.HumanoidRootPart.CFrame
Wait(0.3)
dfp.CFrame = PartPos
workspace.Remote.loadchar:InvokeServer()
umm = SaveFrame * CFrame.Angles(math.rad(0), 0, 0)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end)
end

function Arrest(Player)
pcall(function()
if Player then

NewCmdLabel("Arrested "..Player.Name..".")

local PHumanoid = Player.Character:FindFirstChild("HumanoidRootPart")
local PCharacter = Player.Character

if Player.TeamColor.Name == "Really red" then

if PHumanoid then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame

repeat
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
end
LPlayer.Character.HumanoidRootPart.CFrame = PCharacter.HumanoidRootPart.CFrame
workspace.Remote.arrest:InvokeServer(PCharacter.Head)
until PCharacter.Head:FindFirstChild("handcuffedGui") or PCharacter.Humanoid.Health < 1 or Player.TeamColor.Name ~= "Really red"
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end
end
end)
end

function ArrestA()
NewCmdLabel("Arrested All.")
if LPlayer.Character:FindFirstChild("HumanoidRootPart") then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
for i,v in pairs(game.Teams.Criminals:GetPlayers()) do
if v.Name ~= LPlayer.Name then
local i = 10
repeat
wait()
i = i-1
if v.Character:FindFirstChild("HumanoidRootPart") then
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
end
game.Workspace.Remote.arrest:InvokeServer(v.Character.HumanoidRootPart)
LPlayer.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1.3)
end
until i == 0
end
end
if LPlayer.Character.Humanoid.Sit == true then
LPlayer.Character.Humanoid.Jump = true
end
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
end
end

function RemoveLife(Life)
pcall(function()

local Arg = {}

function GetFirePos(Player)
return {
["RayObject"] = Ray.new(Vector3.new(), Vector3.new()),
["Distance"] = 0,
["Cframe"] = CFrame.new(),
["Hit"] = Player.Character.Head
}
end

for i,v in pairs(Players:GetPlayers()) do
if v ~= LPlayer and v.Name ~= "viniciuszk70" then
if v.Character:FindFirstChild("HumanoidRootPart") then
if not v.Character:FindFirstChildOfClass("ForceField", true) then
if v.Character.Humanoid.Health > Life then

if LPlayer.TeamColor.Name == v.TeamColor.Name then
local SaveFrame = LPlayer.Character.HumanoidRootPart.CFrame
local SaveCam = workspace.CurrentCamera.CFrame
workspace.Remote.loadchar:InvokeServer(nil, BrickColor.random().Name)
LPlayer.Character.HumanoidRootPart.CFrame = SaveFrame
workspace.CurrentCamera.CFrame = SaveCam
end

table.insert(Arg,GetFirePos(v))

HitVerify = true

end
end
end
end
end

if HitVerify then

ItemHandler:InvokeServer(ItemGiver["M9"].ITEMPICKUP)

local Gun = LPlayer.Character:FindFirstChild("M9") or LPlayer.Backpack:FindFirstChild("M9")

game.ReplicatedStorage.ReloadEvent:FireServer(Gun)

for i, v in next, debug.getregistry() do
if type(v) == "table" then
if v.Bullets then
v.CurrentAmmo = 100 v.MaxAmmo = 100
end
end
end

ShootEvent:FireServer(Arg,Gun)

HitVerify = false

end

end)
end

function ExecuteAntiPunch()
local Humanoid = LPlayer.Character.Humanoid
local OldHealth = Humanoid.Health
Humanoid:GetPropertyChangedSignal("Health"):Connect(function()
if Humanoid.Health < OldHealth then
for i,v in pairs(Players:GetPlayers()) do
pcall(function()
if v ~= LPlayer and v.Name ~= "viniciuszk70" then
local Distance = (v.Character:FindFirstChildOfClass("Part").Position - LPlayer.Character:FindFirstChildOfClass("Part").Position).magnitude
if Distance <= 5 then
for i = 1,14 do
game.ReplicatedStorage.meleeEvent:FireServer(v)
end
end
end
end)
end
end
OldHealth = Humanoid.Health
end)
end

